{
    "name": "titasgailius/calendar",
    "description": "Calendar is an intuitive abstraction around various calendar providers.",
    "keywords": ["laravel", "symfony", "calendar", "events", "google", "microsoft"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/TitasGailius/calendar/issues",
        "source": "https://github.com/TitasGailius/calendar"
    },
    "authors": [
        {
            "name": "Titas Gailius",
            "email": "titas.gailius@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "google/apiclient": "^2.12.1",
        "nesbot/carbon": "^2.66"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.0",
        "symfony/var-dumper": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "TitasGailius\\Calendar\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TitasGailius\\Calendar\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "google/apiclient-services": [
            "Calendar"
        ]
    },
    "scripts": {
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
